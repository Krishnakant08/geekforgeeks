     int countWords(string list[], int n)
        {
            
            map<string,int>A;
           for(int i=0;i<n;i++){
               A[list[i]]++;
           }
           
           int count=0;
           
           for(auto x:A){
               if(x.second==2){
                   count++;
               }
           }
           
           return count;
        }
        
