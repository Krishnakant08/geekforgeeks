  int findLongestConseqSubseq(int arr[], int N)
    {
      int sum=1;
      int count=1;
      sort(arr,arr+N);
      for(int i=0;i<N-1;i++){
          if(arr[i]+1==arr[i+1]){
              count++;
          }
          else{
          if(arr[i]==arr[i+1]);
          else
            count=1;
          }
         sum=max(count,sum); 
          
      }
      return sum;
    }
